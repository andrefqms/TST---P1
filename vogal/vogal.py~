palavra = raw_input()
verificador = False
lista1 = ['a','A','e','E','I','i','o','O','u','U']

for i in range(len(palavra)):
    if palavra[i] in lista1:
        print palavra[i]
        verificador = True
        break

if verificador == False:
    print '-'

